{
  "AppSettings": {
    "HeroesProfileApiUrl": "https://api.heroesprofile.com/api",
    "HeroesProfileWebUrl": "https://www.heroesprofile.com",
    "DefaultUserSettings": {
      "WindowTop": 400,
      "WindowLeft": 400,
      "MinimizeToTray": false,
      "WindowHeight": 600,
      "WindowWidth": 700,
      "PreMatchPage": false,
      "PostMatchPage": false
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\temp\\Log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  }
}